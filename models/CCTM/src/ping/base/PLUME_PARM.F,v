C RCS file, release, date & time of last delta, author, state, [and locker]
C $Header: /project/work/rep/CCTM/src/ping/base/PLUME_PARM.F,v 1.5 2005/09/07 17:55:53 jug Exp $ 

C what(1) key, module and SID; SCCS file; date and time of last delta:
C %W% %P% %G% %U%

C:::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::
      MODULE PLUME_PARM

      IMPLICIT NONE

C Contains:  Dimensioning parameters for the Plume-in-Grid module.
 
C Dependent Upon:
 
C Revision History:
C   2/01/2000 Jeff
C     -- f90 memory mgmt
 
C....................................................................

C number of pillars
      INTEGER, PARAMETER :: NPILLARS = 8 
      INTEGER, PARAMETER :: NLEFT = 4 
      INTEGER, PARAMETER :: NRIGHT = NLEFT
C plume release rate per hour.
      INTEGER, PARAMETER :: NRRATE = 1 
C PinG output time step HHMMSS
      INTEGER, PARAMETER :: PINGOUTSTEP = 001200
 
C Simulation control parameters (Switches)
 
C PinG Ground-Removal module switch
      LOGICAL, PARAMETER :: PING_GR_FLG = .TRUE.

C PinG Dispersion module switch
      LOGICAL, PARAMETER :: PING_DS_FLG = .TRUE.

C PinG Diffusion module switch
      LOGICAL, PARAMETER :: PING_DF_FLG = .TRUE.

C PinG Chemistry module switch
      LOGICAL, PARAMETER :: PING_CH_FLG = .TRUE.

C PinG Aerosol module switch
      LOGICAL, PARAMETER :: PING_AE_FLG = .TRUE.

C PinG Hand-Over module switch
      LOGICAL, PARAMETER :: PING_HO_FLG = .TRUE.

C PinG EMISCH flag (Emissions in chem)
      LOGICAL, PARAMETER :: PING_EMISCH = .FALSE.

C Scaling and multiplication factors
 
C Smoothing factor in GETPINGSTEP,  where 0<TSFACTOR<=1
      REAL,    PARAMETER :: PINGSFCTR = 1.0

C Left and right ETA values to be used in subroutine PING_ETA: ETA(i) = Y(i)/Wp
 
      REAL, SAVE :: ETALT( NLEFT+1 )  = ( / 0.0, 0.125, 0.25, 0.375, 0.5 / )
      REAL, SAVE :: ETART( NRIGHT+1 ) = ( / 0.0, 0.125, 0.25, 0.375, 0.5 / )

      REAL, PARAMETER :: PING_XI = 1.5

      END MODULE PLUME_PARM
@


