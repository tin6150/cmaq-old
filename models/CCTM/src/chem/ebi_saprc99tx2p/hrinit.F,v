         EBISP(  35 ) = HCOOH
         EBISP(  36 ) = RCHO
         EBISP(  37 ) = GLY
         EBISP(  38 ) = MGLY
         EBISP(  39 ) = BACL
         EBISP(  40 ) = CRES
         EBISP(  41 ) = BALD
         EBISP(  42 ) = METHACRO
         EBISP(  43 ) = MVK
         EBISP(  44 ) = ISOPROD
         EBISP(  45 ) = CCHO_SUR
         EBISP(  46 ) = DCB1
