C   work in Models-3/CMAQ to the public and to permit others to do     *
C   so.  EPA therefore grants similar permissions for use of the       *
C   Models-3/CMAQ software, but users are requested to provide copies  *
C   of derivative works to the Government without restrictions as to   *
C   use by others.  Users are responsible for acquiring their own      *
C   copies of commercial software associated with Models-3/CMAQ and    *
C   for complying with vendor requirements.  Software copyrights by    *
C   the MCNC Environmental Modeling Center are used with their         *
C   permissions subject to the above restrictions.                     *
C***********************************************************************

C RCS file, release, date & time of last delta, author, state, [and locker]
C $Header: /project/work/rep/CCTM/src/chem/ros3/degrade_data.F,v 1.2 2005/08/30 16:24:52 sjr Exp $

C what(1) key, module and SID; SCCS file; date and time of last delta:
C %W% %P% %G% %U%


      SUBROUTINE DEGRADE_DATA()
C**********************************************************************
C
C  FUNCTION:  Allocate arrays and define data used by degradation routines
C
C    react(i) does not need the maximum number of causes for degradation.
C    Set "un-used" causes to "' '".
C
C    Note, OH and HO both refer to hydroxyl concentrations. The redundancy exists because
C    current mechanisms for gas chemistry use OH or HO to refer to the radical.
C
C  CALLED BY: INIT_DEGRADE
C
C  REVISION HISTORY: : 07/29/05 : B.Hutzell - Initial version
C
C**********************************************************************

      USE DEGRADE_SETUP_TOX

      IMPLICIT NONE

C**********************************************************************

      N_REACT = 16

      N_UNI_LOSS   = 1    ! Number of Unimolecular Loss Processes
      N_BI_LOSS    = 4    ! Number of Bimolecular Loss processes
      N_TRI_LOSS   = 2    ! Number of trimolecular Loss processes
      N_PHOTO_LOSS = 2    ! Number of Photolysis Loss processes

      N_PROCESSES  = N_UNI_LOSS
     &             + N_BI_LOSS
     &             + N_PHOTO_LOSS
     &             + N_TRI_LOSS

C..looping data

      UNI_START   = 1
      BI_START    = UNI_START + N_UNI_LOSS
      TRI_START   = BI_START  + N_BI_LOSS
      PHOTO_START = TRI_START + N_TRI_LOSS

      UNI_STOP   = BI_START    - 1
      BI_STOP    = TRI_START   - 1
      TRI_STOP   = PHOTO_START - 1
      PHOTO_STOP = N_PROCESSES

      ALLOCATE( REACT     ( N_REACT ) )                 ! names of species being degraded
      ALLOCATE( BICAUSE   ( N_REACT, N_BI_LOSS ) )      ! species name that cause degradation
      ALLOCATE( TRICAUSE  ( N_REACT, N_TRI_LOSS, 2 ) )  ! species name that cause degradation
      ALLOCATE( PHOTO_NAME( N_REACT, N_PHOTO_LOSS ) )   ! name of photolysis rate for react(i)

      ALLOCATE( UNIRATE  ( N_REACT, N_UNI_LOSS ) )  ! rate for unimolecular decay for react(i) [molecules/sec^1]
      ALLOCATE( UNI_ACT  ( N_REACT, N_UNI_LOSS ) )  ! activation energy for UNIRATE(I) [K]. Positive if exothermic
      ALLOCATE( UNI_TEXP ( N_REACT, N_UNI_LOSS ) )  ! exponent of Temperature
      ALLOCATE( UNI_PROD ( N_REACT, N_UNI_LOSS ) )  ! name of daughter product for react(i)
      ALLOCATE( UNI_YIELD( N_REACT, N_UNI_LOSS ) )  ! yield of daughter product for react(i)

      ALLOCATE( BIRATE  ( N_REACT, N_BI_LOSS ) )  ! degradation rates for bimolecular reactions,  [cm^3/(sec*molecules)]
      ALLOCATE( BI_ACT  ( N_REACT, N_BI_LOSS ) )  ! activation energy for BIRATE(I) [K]. Positive if exothermic
      ALLOCATE( BI_TEXP ( N_REACT, N_BI_LOSS ) )  ! exponent of Temperature
      ALLOCATE( BI_PROD ( N_REACT, N_BI_LOSS ) )  ! name of daughter product for react(i)
      ALLOCATE( BI_YIELD( N_REACT, N_BI_LOSS ) )  ! yield of daughter product for react(i)

      ALLOCATE( TRIRATE  ( N_REACT, N_TRI_LOSS ) )  ! degradation rates for trimolecular reactions,  [cm^3/(sec*molecules)]
      ALLOCATE( TRI_ACT  ( N_REACT, N_TRI_LOSS ) )  ! activation energy for TRIRATE(I) [K]. Positive if exothermic
      ALLOCATE( TRI_TEXP ( N_REACT, N_TRI_LOSS ) )  ! exponent of Temperature
      ALLOCATE( TRI_PROD ( N_REACT, N_TRI_LOSS ) )  ! name of daughter product for react(i)
      ALLOCATE( TRI_YIELD( N_REACT, N_TRI_LOSS ) )  ! yield of daughter product for react(i)

      ALLOCATE( A_PHOTO    ( N_REACT, N_PHOTO_LOSS ) )  ! multiplier of photolysis rates
      ALLOCATE( PHOTO_PROD ( N_REACT, N_PHOTO_LOSS ) )  ! name of daughter product for react(i)
      ALLOCATE( PHOTO_YIELD( N_REACT, N_PHOTO_LOSS ) )  ! yield of daughter product for react(i)

      ALLOCATE( RATE_CONST( BLKSIZE, N_REACT, N_PROCESSES ) )
      ALLOCATE( RATE_YIELD( N_REACT, N_PROCESSES ) )

C..arrays to store indices to CGRID

      ALLOCATE( RXTANT_MAP( N_REACT ) )
      ALLOCATE( PROD_MAP  ( N_REACT, N_PROCESSES ) )
      ALLOCATE( RAD_MAP   ( N_REACT, N_BI_LOSS ) )
      ALLOCATE( RAD2_MAP  ( N_REACT, N_TRI_LOSS, 2 ) )
      ALLOCATE( PHOTO_MAP ( N_REACT, N_PHOTO_LOSS ) )

      REACT      = BLANK
      UNI_PROD   = BLANK
      BI_PROD    = BLANK
      TRI_PROD   = BLANK
      PHOTO_PROD = BLANK
      PHOTO_NAME = BLANK
      BICAUSE    = BLANK
      TRICAUSE   = BLANK

      UNI_ACT   = 0.0D0
      UNIRATE   = 0.0D0
      UNI_TEXP  = 0.0
      UNI_YIELD = 0.0

      BI_ACT   = 0.0D0
      BIRATE   = 0.0D0
      BI_TEXP  = 0.0
      BI_YIELD = 0.0

      TRI_ACT   = 0.0D0
      TRIRATE   = 0.0D0
      TRI_TEXP  = 0.0
      TRI_YIELD = 0.0

      A_PHOTO     = 0.0D0
      PHOTO_YIELD = 0.0

      RATE_YIELD = 1.0D0

C..Initialize maps

      RXTANT_MAP = -1
      RAD_MAP    = -1
      RAD2_MAP   = -1
      PROD_MAP   = -1
      PHOTO_MAP  = -1

C..toxic VOC's

C.. ACRYLONITRILE

      REACT(1)       = 'ACRYLONITRILE'
      UNIRATE( 1, 1) = 0.0D0
      UNI_ACT( 1, 1) = 0.0D0
      UNI_TEXP(1, 1) = 0.0
      BICAUSE(1,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(1,1:N_BI_LOSS)  = (/ 4.8D-12, 4.8D-12, 1.9D-19, 6.3D-18/)
      BI_ACT(1,1:N_BI_LOSS)  = (/   0.0D0,   0.0D0, 0.0D0, 0.0D0/)
      BI_TEXP(1,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. CARBON TETRACHLORIDE

      REACT(2)       = 'CARBONTET'
      UNIRATE( 2, 1) = 0.0D0
      UNI_ACT( 2, 1) = 0.0D0
      UNI_TEXP(2, 1) = 0.0
      BICAUSE(2,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(2,1:N_BI_LOSS)  = (/ 1.0D-12, 1.0D-12, 0.0D0, 0.0D0/)
      BI_ACT(2,1:N_BI_LOSS)  = (/2300.0D0,2300.0D0, 0.0D0, 0.0D0/)
      BI_TEXP(2,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. PROPYLENE DICHLORIDE

      REACT(3)       = 'PROPDICHLORIDE'
      UNIRATE( 3, 1) = 0.0D0
      UNI_ACT( 3, 1) = 0.0D0
      UNI_TEXP(3, 1) = 0.0
      BICAUSE(3,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(3,1:N_BI_LOSS)  = (/ 2.2D-13, 2.2D-13, 0.0D0, 0.0D0/)
      BI_ACT(3,1:N_BI_LOSS)  = (/   0.0D0,   0.0D0, 0.0D0, 0.0D0/)
      BI_TEXP(3,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. DICHLORIPROPENE

      REACT(4)       = 'DICHLOROPROPENE'
      UNIRATE( 4, 1) = 0.0D0
      UNI_ACT( 4, 1) = 0.0D0
      UNI_TEXP(4, 1) = 0.0
      BICAUSE(4,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(4,1:N_BI_LOSS)  = (/1.04D-11, 1.04D-11, 4.10D-19, 0.0D0/)
      BI_ACT(4,1:N_BI_LOSS)  = (/   0.0D0,    0.0D0, 0.0D0, 0.0D0/)
      BI_TEXP(4,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. 1,1,2,2TETRACHLOROETHANE

      REACT(5)       = 'CL4_ETHANE1122'
      UNIRATE( 5, 1) = 0.0D0
      UNI_ACT( 5, 1) = 0.0D0
      UNI_TEXP(5, 1) = 0.0
      BICAUSE(5,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(5,1:N_BI_LOSS)  = (/2.46D-13, 2.46D-13, 0.0D0, 0.0D0/)
      BI_ACT(5,1:N_BI_LOSS)  = (/   0.0D0,    0.0D0, 0.0D0, 0.0D0/)
      BI_TEXP(5,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. BENZENE

      REACT(6)       = 'BENZENE'
      UNIRATE( 6, 1) = 0.0D0
      UNI_ACT( 6, 1) = 0.0D0
      UNI_TEXP(6, 1) = 0.0
      BICAUSE(6,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(6,1:N_BI_LOSS)  = (/ 2.47D-12, 2.47D-12, 0.0D0, 0.0D0/)
      BI_ACT(6,1:N_BI_LOSS)  = (/  207.0D0,  207.0D0, 0.0D0, 0.0D0/)
      BI_TEXP(6,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. CHLOROFORM

      REACT(7)       = 'CHCL3'
      UNIRATE( 7, 1) = 0.0D0
      UNI_ACT( 7, 1) = 0.0D0
      UNI_TEXP(7, 1) = 0.0
      BICAUSE(7,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(7,1:N_BI_LOSS)  = (/6.30D-18, 6.30D-18, 0.0D0, 0.0D0/)
      BI_ACT(7,1:N_BI_LOSS)  = (/  504.D0,   504.D0, 0.0D0, 0.0D0/)
      BI_TEXP(7,1:N_BI_LOSS) = (/      2.,       2., 0., 0./)

C.. 1,2DIBROMOETHANE

      REACT(8)       = 'BR2_C2_12'
      UNIRATE( 8, 1) = 0.0D0
      UNI_ACT( 8, 1) = 0.0D0
      UNI_TEXP(8, 1) = 0.0
      BICAUSE(8,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(8,1:N_BI_LOSS)  = (/1.03D-17, 1.03D-17, 0.0D0, 0.0D0/)
      BI_ACT(8,1:N_BI_LOSS)  = (/  422.D0,   422.D0, 0.0D0, 0.0D0/)
      BI_TEXP(8,1:N_BI_LOSS) = (/      2.,       2., 0., 0./)

C.. 1,2DICHLOROTHANE

      REACT(9)       = 'CL2_C2_12'
      UNIRATE( 9, 1) = 0.0D0
      UNI_ACT( 9, 1) = 0.0D0
      UNI_TEXP(9, 1) = 0.0
      BICAUSE(9,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(9,1:N_BI_LOSS)  = (/1.10D-17, 1.10D-17, 0.0D0, 0.0D0/)
      BI_ACT(9,1:N_BI_LOSS)  = (/  409.D0,   409.D0, 0.0D0, 0.0D0/)
      BI_TEXP(9,1:N_BI_LOSS) = (/      2.,       2., 0., 0./)

C.. ETHYLENE OXIDE

      REACT(10)       = 'ETOX'
      UNIRATE( 10, 1) = 0.0D0
      UNI_ACT( 10, 1) = 0.0D0
      UNI_TEXP(10, 1) = 0.0
      BICAUSE(10,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(10,1:N_BI_LOSS)  = (/7.60D-14, 7.60D-14, 0.0D0, 0.0D0/)
      BI_ACT(10,1:N_BI_LOSS)  = (/   0.0D0,    0.0D0, 0.0D0, 0.0D0/)
      BI_TEXP(10,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. METHYLENE CHLORIDE

      REACT(11)       = 'CL2_ME'
      UNIRATE( 11, 1) = 0.0D0
      UNI_ACT( 11, 1) = 0.0D0
      UNI_TEXP(11, 1) = 0.0
      BICAUSE(11,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(11,1:N_BI_LOSS)  = (/8.54D-18, 8.54D-18, 0.0D0, 0.0D0/)
      BI_ACT(11,1:N_BI_LOSS)  = (/  500.D0,   500.D0, 0.0D0, 0.0D0/)
      BI_TEXP(11,1:N_BI_LOSS) = (/      2.,      2., 0., 0./)

C.. PERCHLOROETHYLENE

      REACT(12)       = 'CL4_ETHE'
      UNIRATE(12, 1)  = 0.0D0
      UNI_ACT( 12, 1) = 0.0D0
      UNI_TEXP(12, 1) = 0.0
      BICAUSE(12,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(12,1:N_BI_LOSS)  = (/9.64D-12, 9.64D-12, 0.0D0, 0.0D0/)
      BI_ACT(12,1:N_BI_LOSS)  = (/ 1209.D0,  1209.D0, 0.0D0, 0.0D0/)
      BI_TEXP(12,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. TRICHLOROETHYLENE

      REACT(13)       = 'CL3_ETHE'
      UNIRATE( 13, 1) = 0.0D0
      UNI_ACT( 13, 1) = 0.0D0
      UNI_TEXP(13, 1) = 0.0
      BICAUSE(13,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(13,1:N_BI_LOSS)  = (/5.63D-13, 5.63D-13, 0.0D0, 0.0D0/)
      BI_ACT(13,1:N_BI_LOSS)  = (/ -427.D0,  -427.D0, 0.0D0, 0.0D0/)
      BI_TEXP(13,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. VINYL CHLORIDE

      REACT(14)       = 'CL_ETHE'
      UNIRATE(14, 1)  = 0.0D0
      UNI_ACT( 14, 1) = 0.0D0
      UNI_TEXP(14, 1) = 0.0
      BICAUSE(14,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO3'/)
      BIRATE(14,1:N_BI_LOSS)  = (/1.69D-12, 1.69D-12, 0.0D0,1.84D-13/)
      BI_ACT(14,1:N_BI_LOSS)  = (/ -422.D0,  -422.D0, 0.0D0, 1764.D0/)
      BI_TEXP(14,1:N_BI_LOSS) = (/      0.,       0., 0., 0./)

C.. NAPHTHALENE

      REACT(15)       = 'NAPHTHALENE'
      BICAUSE(15,1:3) = (/'OH ','HO ','O3 '/)
      BIRATE(15,1:3)  = (/1.05D-12, 1.05D-12, 2.01D-19/)
      BI_ACT(15,1:3)  = (/ -901.D0,  -901.D0,    0.0D0/)
      TRICAUSE(15,1,1:2) = (/'NO3 ','NO2 '/)
      TRIRATE( 15,1)     =  3.94D-34
      TRI_ACT( 15,1)     = -4.10D+03

C.. QUINILINE

      REACT(16)       = 'QUINOLINE'
      BICAUSE(16,1:N_BI_LOSS) = (/'OH ','HO ','O3 ','NO2'/)
      BIRATE(16,1:N_BI_LOSS)  = (/1.16D-11, 1.16D-11, 1.00D-19, 1.60D-20/)
      BI_ACT(16,1:N_BI_LOSS)  = (/   0.0D0,    0.0D0,    0.0D0,    0.0D0/)

      RETURN
      END
