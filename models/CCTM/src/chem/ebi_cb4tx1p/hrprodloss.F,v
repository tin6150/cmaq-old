C RCS file, release, date & time of last delta, author, state, [and locker]
C $Header: /project/work/rep/CCTM/src/chem/ebi_cb4tx1p/hrprodloss.F,v 1.1.1.1 2005/09/01 20:22:46 sjr Exp $ 

C what(1) key, module and SID; SCCS file; date and time of last delta:
C %W% %P% %G% %U%


      SUBROUTINE HRPRODLOSS

C***********************************************************************
C
C  FUNCTION: To compute the production and loss rates for EBI species
C
C  PRECONDITIONS: For the CB4TX1P family of mechanisms
C
C  RETURN VALUES: None
C
C  KEY SUBROUTINES/FUNCTIONS CALLED: None
C
C  REVISION HISTORY: Created by EBI solver program, Aug. 30, 2005
C
C***********************************************************************
      USE HRDATA

      IMPLICIT NONE


C.....INCLUDES: NONE


C.....ARGUMENTS: NONE


C.....PARAMETERS: NONE


C.....EXTERNAL FUNCTIONS: NONE


C.....SAVED VARIABLES: NONE
      CHARACTER( 16 ), SAVE  ::  PNAME = 'HRPRODLOSS'   ! Program name


C.....LOCAL VARIABLES:

      INTEGER ESP

C***********************************************************************


      PROD( HNO3             ) =   2.000 * RXRAT(  19 )      ! N2O5=2*HNO3
     &                          +          RXRAT(  27 )      ! OH+NO2=HNO3
     &                          +          RXRAT(  42 )      ! FORM+NO3=HNO3+HO2+CO
     &                          +          RXRAT(  45 )      ! ALD2+NO3=HNO3+C2O3
     &                          +          RXRAT(  68 )      ! CRES+NO3=HNO3+CRO+CSLAER
     &                          +  0.075 * RXRAT(  91 )      ! ISPD+NO3=0.075*HNO3+...
     &                          +  0.851 * RXRAT(  96 )      ! ACROLEIN+NO3=0.851*HNO3+...
     &                          +          RXRAT( 105 )      ! ALD2_SUR+NO3=HNO3+C2O3
     &                          +          RXRAT( 111 )      ! FORM_SUR+NO3=HNO3+HO2+CO
      LOSS( HNO3             ) =           RXRAT(  28 )      ! HNO3+OH=NO3

      PROD( H2O2             ) =           RXRAT(  33 )      ! HO2+HO2=H2O2
     &                          +          RXRAT(  34 )      ! HO2+HO2=H2O2
      LOSS( H2O2             ) =           RXRAT(  35 )      ! H2O2=2*OH
     &                          +          RXRAT(  36 )      ! H2O2+OH=HO2

      PROD( CO               ) =           RXRAT(  38 )      ! FORM+OH=CO+HO2
     &                          +          RXRAT(  39 )      ! FORM=CO+2*HO2
     &                          +          RXRAT(  40 )      ! FORM=CO
     &                          +          RXRAT(  41 )      ! FORM+O=CO+HO2+OH
     &                          +          RXRAT(  42 )      ! FORM+NO3=CO+HO2+HNO3
     &                          +          RXRAT(  46 )      ! ALD2=CO+2*HO2+XO2+FORM
     &                          +  0.300 * RXRAT(  57 )      ! OLE+O=0.3*CO+0.38*HO2+...
     &                          +  0.330 * RXRAT(  59 )      ! OLE+O3=0.33*CO+0.74*FORM+...
     &                          +          RXRAT(  61 )      ! ETH+O=CO+0.7*XO2+FORM+...
     &                          +  0.420 * RXRAT(  63 )      ! ETH+O3=0.42*CO+FORM+...
     &                          +  2.000 * RXRAT(  71 )      ! OPEN+OH=2*CO+XO2+2*HO2+...
     &                          +          RXRAT(  72 )      ! OPEN=CO+HO2+C2O3
     &                          +  0.690 * RXRAT(  73 )      ! OPEN+O3=0.69*CO+0.62*C2O3+...
     &                          +          RXRAT(  75 )      ! MGLY=CO+HO2+C2O3
     &                          +  0.066 * RXRAT(  78 )      ! ISOP+O3=0.066*CO+0.6*FORM+...
     &                          +  0.334 * RXRAT(  89 )      ! ISPD+OH=0.334*CO+...
     &                          +  0.225 * RXRAT(  90 )      ! ISPD+O3=0.225*CO+0.15*FORM+...
     &                          +  0.643 * RXRAT(  91 )      ! ISPD+NO3=0.643*CO+...
     &                          +  0.333 * RXRAT(  92 )      ! ISPD=0.333*CO+...
     &                          +  6.815 * RXRAT(  95 )      ! ACROLEIN+O3=6.815*CO+...
     &                          +  0.257 * RXRAT(  97 )      ! ACROLEIN+O=0.257*CO+...
     &                          +  6.300 * RXRAT(  98 )      ! ACROLEIN=6.3*CO+12.6*HO2+...
     &                          +  1.260 * RXRAT( 100 )      ! BUTADIENE13+O3=1.26*CO+...
     &                          +  0.123 * RXRAT( 102 )      ! BUTADIENE13+O=0.123*CO+...
     &                          +          RXRAT( 106 )      ! ALD2_SUR=CO+2*HO2+XO2+FORM
     &                          +          RXRAT( 107 )      ! FORM_SUR+OH=CO+HO2
     &                          +          RXRAT( 108 )      ! FORM_SUR=CO+2*HO2
     &                          +          RXRAT( 109 )      ! FORM_SUR=CO
     &                          +          RXRAT( 110 )      ! FORM_SUR+O=CO+HO2+OH
     &                          +          RXRAT( 111 )      ! FORM_SUR+NO3=CO+HO2+HNO3
      LOSS( CO               ) =           RXRAT(  37 )      ! CO+OH=HO2

      PROD( FORM             ) =           RXRAT(  46 )      ! ALD2=FORM+2*HO2+CO+XO2
     &                          +          RXRAT(  47 )      ! C2O3+NO=FORM+XO2+NO2+HO2
     &                          +  2.000 * RXRAT(  50 )      ! C2O3+C2O3=2*FORM+2*XO2+2*HO2
     &                          +  0.790 * RXRAT(  51 )      ! C2O3+HO2=0.79*FORM+...
     &                          +          RXRAT(  52 )      ! OH=FORM+XO2+HO2
     &                          +  0.200 * RXRAT(  57 )      ! OLE+O=0.2*FORM+0.38*HO2+...
     &                          +          RXRAT(  58 )      ! OLE+OH=FORM+ALD2+XO2+HO2-PAR
     &                          +  0.740 * RXRAT(  59 )      ! OLE+O3=0.74*FORM+0.5*ALD2+...
     &                          +          RXRAT(  60 )      ! OLE+NO3=FORM+0.09*XO2N+...
     &                          +          RXRAT(  61 )      ! ETH+O=FORM+0.7*XO2+CO+...
     &                          +  1.560 * RXRAT(  62 )      ! ETH+OH=1.56*FORM+XO2+HO2+...
     &                          +          RXRAT(  63 )      ! ETH+O3=FORM+0.42*CO+...
     &                          +          RXRAT(  71 )      ! OPEN+OH=FORM+2*CO+2*HO2+...
     &                          +  0.700 * RXRAT(  73 )      ! OPEN+O3=0.7*FORM+0.62*C2O3+...
     &                          +  0.500 * RXRAT(  76 )      ! ISOP+O=0.5*FORM+0.75*ISPD+...
     &                          +  0.629 * RXRAT(  77 )      ! ISOP+OH=0.629*FORM+...
     &                          +  0.600 * RXRAT(  78 )      ! ISOP+O3=0.6*FORM+0.65*ISPD+...
     &                          +  0.167 * RXRAT(  89 )      ! ISPD+OH=0.167*FORM+...
     &                          +  0.150 * RXRAT(  90 )      ! ISPD+O3=0.15*FORM+...
     &                          +  0.282 * RXRAT(  91 )      ! ISPD+NO3=0.282*FORM+...
     &                          +  0.900 * RXRAT(  92 )      ! ISPD=0.9*FORM+...
     &                          +  0.709 * RXRAT(  94 )      ! ACROLEIN+OH=0.709*FORM+...
     &                          + 15.282 * RXRAT(  95 )      ! ACROLEIN+O3=15.282*FORM+...
     &                          +  1.310 * RXRAT(  96 )      ! ACROLEIN+NO3=1.31*FORM+...
     &                          +  0.171 * RXRAT(  97 )      ! ACROLEIN+O=0.171*FORM+...
     &                          +  6.300 * RXRAT(  98 )      ! ACROLEIN=6.3*FORM+12.6*HO2+...
     &                          +  0.850 * RXRAT(  99 )      ! BUTADIENE13+OH=0.85*FORM+...
     &                          +  2.810 * RXRAT( 100 )      ! BUTADIENE13+O3=2.81*FORM+...
     &                          +  0.154 * RXRAT( 101 )      ! BUTADIENE13+NO3=0.154*FORM+...
     &                          +  0.082 * RXRAT( 102 )      ! BUTADIENE13+O=0.082*FORM+...
     &                          +          RXRAT( 106 )      ! ALD2_SUR=FORM+2*HO2+CO+XO2
      LOSS( FORM             ) =           RXRAT(  38 )      ! FORM+OH=HO2+CO
     &                          +          RXRAT(  39 )      ! FORM=2*HO2+CO
     &                          +          RXRAT(  40 )      ! FORM=CO
     &                          +          RXRAT(  41 )      ! FORM+O=OH+HO2+CO
     &                          +          RXRAT(  42 )      ! FORM+NO3=HNO3+HO2+CO

      PROD( ALD2             ) =   0.110 * RXRAT(  53 )      ! PAR+OH=0.11*ALD2+0.13*XO2N+...
     &                          +  1.100 * RXRAT(  54 )      ! ROR=1.1*ALD2+0.96*XO2+...
     &                          +  0.630 * RXRAT(  57 )      ! OLE+O=0.63*ALD2+0.38*HO2+...
     &                          +          RXRAT(  58 )      ! OLE+OH=ALD2+FORM+XO2+HO2-PAR
     &                          +  0.500 * RXRAT(  59 )      ! OLE+O3=0.5*ALD2+0.74*FORM+...
     &                          +          RXRAT(  60 )      ! OLE+NO3=ALD2+0.09*XO2N+...
     &                          +  0.030 * RXRAT(  73 )      ! OPEN+O3=0.03*ALD2+...
     &                          +  0.800 * RXRAT(  79 )      ! ISOP+NO3=0.8*ALD2+0.8*NTR+...
     &                          +  0.273 * RXRAT(  89 )      ! ISPD+OH=0.273*ALD2+...
     &                          +  0.020 * RXRAT(  90 )      ! ISPD+O3=0.02*ALD2+...
     &                          +  0.800 * RXRAT(  93 )      ! ISOP+NO2=0.8*ALD2+0.8*NTR+...
      LOSS( ALD2             ) =           RXRAT(  43 )      ! ALD2+O=C2O3+OH
     &                          +          RXRAT(  44 )      ! ALD2+OH=C2O3
     &                          +          RXRAT(  45 )      ! ALD2+NO3=C2O3+HNO3
     &                          +          RXRAT(  46 )      ! ALD2=XO2+2*HO2+CO+FORM

      PROD( XO2              ) =           RXRAT(  46 )      ! ALD2=XO2+2*HO2+CO+FORM
     &                          +          RXRAT(  47 )      ! C2O3+NO=XO2+NO2+FORM+HO2
     &                          +  2.000 * RXRAT(  50 )      ! C2O3+C2O3=2*XO2+2*FORM+2*HO2
     &                          +  0.790 * RXRAT(  51 )      ! C2O3+HO2=0.79*XO2+...
     &                          +          RXRAT(  52 )      ! OH=XO2+FORM+HO2
     &                          +  0.870 * RXRAT(  53 )      ! PAR+OH=0.87*XO2+0.13*XO2N+...
     &                          +  0.960 * RXRAT(  54 )      ! ROR=0.96*XO2+1.1*ALD2+...
     &                          +  0.280 * RXRAT(  57 )      ! OLE+O=0.28*XO2+0.38*HO2+...
     &                          +          RXRAT(  58 )      ! OLE+OH=XO2+ALD2+FORM+HO2-PAR
     &                          +  0.220 * RXRAT(  59 )      ! OLE+O3=0.22*XO2+0.74*FORM+...
     &                          +  0.910 * RXRAT(  60 )      ! OLE+NO3=0.91*XO2+0.09*XO2N+...
     &                          +  0.700 * RXRAT(  61 )      ! ETH+O=0.7*XO2+FORM+CO+...
     &                          +          RXRAT(  62 )      ! ETH+OH=XO2+1.56*FORM+HO2+...
     &                          +  0.080 * RXRAT(  64 )      ! TOL+OH=0.08*XO2+0.36*CRES+...
     &                          +  0.600 * RXRAT(  67 )      ! CRES+OH=0.6*XO2+0.4*CRO+...
     &                          +  0.500 * RXRAT(  70 )      ! XYL+OH=0.5*XO2+0.7*HO2+...
     &                          +          RXRAT(  71 )      ! OPEN+OH=XO2+2*CO+2*HO2+...
     &                          +  0.030 * RXRAT(  73 )      ! OPEN+O3=0.03*XO2+0.62*C2O3+...
     &                          +          RXRAT(  74 )      ! MGLY+OH=XO2+C2O3
     &                          +  0.250 * RXRAT(  76 )      ! ISOP+O=0.25*XO2+0.5*FORM+...
     &                          +  0.991 * RXRAT(  77 )      ! ISOP+OH=0.991*XO2+...
     &                          +  0.200 * RXRAT(  78 )      ! ISOP+O3=0.2*XO2+0.6*FORM+...
     &                          +          RXRAT(  79 )      ! ISOP+NO3=XO2+0.8*NTR+...
     &                          +  0.713 * RXRAT(  89 )      ! ISPD+OH=0.713*XO2+...
     &                          +  0.064 * RXRAT(  90 )      ! ISPD+O3=0.064*XO2+...
     &                          +  0.075 * RXRAT(  91 )      ! ISPD+NO3=0.075*XO2+...
     &                          +  0.700 * RXRAT(  92 )      ! ISPD=0.7*XO2+...
     &                          +          RXRAT(  93 )      ! ISOP+NO2=XO2+0.8*NTR+...
     &                          +  0.709 * RXRAT(  94 )      ! ACROLEIN+OH=0.709*XO2+...
     &                          +  4.543 * RXRAT(  95 )      ! ACROLEIN+O3=4.543*XO2+...
     &                          +  1.192 * RXRAT(  96 )      ! ACROLEIN+NO3=1.192*XO2+...
     &                          +  0.239 * RXRAT(  97 )      ! ACROLEIN+O=0.239*XO2+...
     &                          +  6.300 * RXRAT(  98 )      ! ACROLEIN=6.3*XO2+12.6*HO2+...
     &                          +  0.850 * RXRAT(  99 )      ! BUTADIENE13+OH=0.85*XO2+...
     &                          +  0.840 * RXRAT( 100 )      ! BUTADIENE13+O3=0.84*XO2+...
     &                          +  0.140 * RXRAT( 101 )      ! BUTADIENE13+NO3=0.14*XO2+...
     &                          +  0.115 * RXRAT( 102 )      ! BUTADIENE13+O=0.115*XO2+...
     &                          +          RXRAT( 106 )      ! ALD2_SUR=XO2+2*HO2+CO+FORM
      LOSS( XO2              ) =           RXRAT(  80 )      ! XO2+NO=NO2
     &                          +  2.000 * RXRAT(  81 )      ! XO2+XO2=
     &                          +          RXRAT(  85 )      ! XO2+HO2=UMHP
     &                          +          RXRAT(  88 )      ! XO2+XO2N=

      PROD( PACD             ) =   0.210 * RXRAT(  51 )      ! C2O3+HO2=0.21*PACD+...
      LOSS( PACD             ) =   0.0

      PROD( PAR              ) =   0.220 * RXRAT(  57 )      ! OLE+O=0.22*PAR+0.38*HO2+...
     &                          +  1.100 * RXRAT(  70 )      ! XYL+OH=1.1*PAR+0.5*XO2+...
     &                          +  0.250 * RXRAT(  76 )      ! ISOP+O=0.25*PAR+0.5*FORM+...
     &                          +  0.350 * RXRAT(  78 )      ! ISOP+O3=0.35*PAR+0.6*FORM+...
     &                          +  2.400 * RXRAT(  79 )      ! ISOP+NO3=2.4*PAR+0.8*NTR+...
     &                          +  1.565 * RXRAT(  89 )      ! ISPD+OH=1.565*PAR+...
     &                          +  0.360 * RXRAT(  90 )      ! ISPD+O3=0.36*PAR+0.15*FORM+...
     &                          +  1.282 * RXRAT(  91 )      ! ISPD+NO3=1.282*PAR+...
     &                          +  0.832 * RXRAT(  92 )      ! ISPD=0.832*PAR+...
     &                          +  2.400 * RXRAT(  93 )      ! ISOP+NO2=2.4*PAR+0.8*NTR+...
     &                          +  0.188 * RXRAT(  97 )      ! ACROLEIN+O=0.188*PAR+...
     &                          +  0.090 * RXRAT( 102 )      ! BUTADIENE13+O=0.09*PAR+...
      LOSS( PAR              ) =   1.110 * RXRAT(  53 )      ! PAR+OH=-0.11*PAR+0.13*XO2N+...
     &                          +  2.100 * RXRAT(  54 )      ! ROR=-2.1*PAR+0.96*XO2+...
     &                          +          RXRAT(  58 )      ! OLE+OH=-PAR+ALD2+XO2+HO2+FORM
     &                          +          RXRAT(  59 )      ! OLE+O3=-PAR+0.74*FORM+...
     &                          +          RXRAT(  60 )      ! OLE+NO3=-PAR+0.09*XO2N+...
     &                          +  0.709 * RXRAT(  94 )      ! ACROLEIN+OH=-0.709*PAR+...
     &                          + 20.651 * RXRAT(  95 )      ! ACROLEIN+O3=-20.651*PAR+...
     &                          +  1.310 * RXRAT(  96 )      ! ACROLEIN+NO3=-1.31*PAR+...
     &                          +  0.850 * RXRAT(  99 )      ! BUTADIENE13+OH=-0.85*PAR+...
     &                          +  3.800 * RXRAT( 100 )      ! BUTADIENE13+O3=-3.8*PAR+...
     &                          +  0.154 * RXRAT( 101 )      ! BUTADIENE13+NO3=-0.154*PAR+...

      PROD( XO2N             ) =   0.130 * RXRAT(  53 )      ! PAR+OH=0.13*XO2N+0.87*XO2+...
     &                          +  0.040 * RXRAT(  54 )      ! ROR=0.04*XO2N+0.96*XO2+...
     &                          +  0.020 * RXRAT(  57 )      ! OLE+O=0.02*XO2N+0.38*HO2+...
     &                          +  0.090 * RXRAT(  60 )      ! OLE+NO3=0.09*XO2N+0.91*XO2+...
     &                          +  0.088 * RXRAT(  77 )      ! ISOP+OH=0.088*XO2N+...
     &                          +  0.118 * RXRAT(  96 )      ! ACROLEIN+NO3=0.118*XO2N+...
     &                          +  0.017 * RXRAT(  97 )      ! ACROLEIN+O=0.017*XO2N+...
     &                          +  0.014 * RXRAT( 101 )      ! BUTADIENE13+NO3=0.014*XO2N+...
     &                          +  0.008 * RXRAT( 102 )      ! BUTADIENE13+O=0.008*XO2N+...
      LOSS( XO2N             ) =           RXRAT(  82 )      ! XO2N+NO=NTR
     &                          +          RXRAT(  86 )      ! XO2N+HO2=
     &                          +  2.000 * RXRAT(  87 )      ! XO2N+XO2N=
     &                          +          RXRAT(  88 )      ! XO2N+XO2=

      PROD( ROR              ) =   0.760 * RXRAT(  53 )      ! PAR+OH=0.76*ROR+0.13*XO2N+...
      LOSS( ROR              ) =   0.980 * RXRAT(  54 )      ! ROR=0.02*ROR+0.96*XO2+...
     &                          +          RXRAT(  55 )      ! ROR=HO2
     &                          +          RXRAT(  56 )      ! ROR+NO2=NTR

      PROD( NTR              ) =           RXRAT(  56 )      ! ROR+NO2=NTR
     &                          +  0.100 * RXRAT(  65 )      ! TO2+NO=0.1*NTR+0.9*HO2+...
     &                          +          RXRAT(  69 )      ! CRO+NO2=NTR
     &                          +  0.800 * RXRAT(  79 )      ! ISOP+NO3=0.8*NTR+0.2*ISPD+...
     &                          +          RXRAT(  82 )      ! XO2N+NO=NTR
     &                          +  0.850 * RXRAT(  91 )      ! ISPD+NO3=0.85*NTR+...
     &                          +  0.800 * RXRAT(  93 )      ! ISOP+NO2=0.8*NTR+0.2*ISPD+...
      LOSS( NTR              ) =   0.0

      PROD( OLE              ) =   0.0
      LOSS( OLE              ) =           RXRAT(  57 )      ! OLE+O=0.63*ALD2+0.38*HO2+...
     &                          +          RXRAT(  58 )      ! OLE+OH=FORM+ALD2+XO2+HO2-PAR
     &                          +          RXRAT(  59 )      ! OLE+O3=0.5*ALD2+0.74*FORM+...
     &                          +          RXRAT(  60 )      ! OLE+NO3=0.91*XO2+0.09*XO2N+...

      PROD( FACD             ) =   0.200 * RXRAT(  59 )      ! OLE+O3=0.2*FACD+0.74*FORM+...
     &                          +  0.400 * RXRAT(  63 )      ! ETH+O3=0.4*FACD+0.42*CO+...
     &                          +  4.130 * RXRAT(  95 )      ! ACROLEIN+O3=4.13*FACD+...
     &                          +  0.760 * RXRAT( 100 )      ! BUTADIENE13+O3=0.76*FACD+...
      LOSS( FACD             ) =   0.0

      PROD( AACD             ) =   0.200 * RXRAT(  59 )      ! OLE+O3=0.2*AACD+0.74*FORM+...
     &                          +  4.130 * RXRAT(  95 )      ! ACROLEIN+O3=4.13*AACD+...
     &                          +  0.760 * RXRAT( 100 )      ! BUTADIENE13+O3=0.76*AACD+...
      LOSS( AACD             ) =   0.0

      PROD( ETH              ) =   0.0
      LOSS( ETH              ) =           RXRAT(  61 )      ! ETH+O=FORM+0.7*XO2+CO+...
     &                          +          RXRAT(  62 )      ! ETH+OH=XO2+1.56*FORM+HO2+...
     &                          +          RXRAT(  63 )      ! ETH+O3=FORM+0.42*CO+...

      PROD( ALD2_SUR         ) =   0.220 * RXRAT(  62 )      ! ETH+OH=0.22*ALD2_SUR+...
     &                          +  0.150 * RXRAT(  78 )      ! ISOP+O3=0.15*ALD2_SUR+...
     &                          +  0.357 * RXRAT(  91 )      ! ISPD+NO3=0.357*ALD2_SUR+...
     &                          +  0.067 * RXRAT(  92 )      ! ISPD=0.067*ALD2_SUR+...
     &                          +  0.709 * RXRAT(  94 )      ! ACROLEIN+OH=0.709*ALD2_SUR+...
     &                          + 10.326 * RXRAT(  95 )      ! ACROLEIN+O3=...
     &                          +  1.310 * RXRAT(  96 )      ! ACROLEIN+NO3=1.31*ALD2_SUR+...
     &                          +  0.538 * RXRAT(  97 )      ! ACROLEIN+O=0.538*ALD2_SUR+...
     &                          +  0.154 * RXRAT( 101 )      ! BUTADIENE13+NO3=...
      LOSS( ALD2_SUR         ) =           RXRAT( 103 )      ! ALD2_SUR+O=C2O3+OH
     &                          +          RXRAT( 104 )      ! ALD2_SUR+OH=C2O3
     &                          +          RXRAT( 105 )      ! ALD2_SUR+NO3=C2O3+HNO3
     &                          +          RXRAT( 106 )      ! ALD2_SUR=XO2+2*HO2+CO+FORM

      PROD( TOL              ) =   0.0
      LOSS( TOL              ) =           RXRAT(  64 )      ! TOL+OH=0.08*XO2+0.36*CRES+...

      PROD( CRES             ) =   0.360 * RXRAT(  64 )      ! TOL+OH=0.36*CRES+0.08*XO2+...
     &                          +          RXRAT(  66 )      ! TO2=CRES+HO2
     &                          +  0.200 * RXRAT(  70 )      ! XYL+OH=0.2*CRES+0.5*XO2+...
      LOSS( CRES             ) =           RXRAT(  67 )      ! CRES+OH=0.4*CRO+0.6*XO2+...
     &                          +          RXRAT(  68 )      ! CRES+NO3=CRO+HNO3+CSLAER

      PROD( TO2              ) =   0.560 * RXRAT(  64 )      ! TOL+OH=0.56*TO2+0.36*CRES+...
     &                          +  0.300 * RXRAT(  70 )      ! XYL+OH=0.3*TO2+0.5*XO2+...
      LOSS( TO2              ) =           RXRAT(  65 )      ! TO2+NO=0.9*NO2+0.9*HO2+...
     &                          +          RXRAT(  66 )      ! TO2=CRES+HO2

      PROD( OPEN             ) =   0.900 * RXRAT(  65 )      ! TO2+NO=0.9*OPEN+0.9*HO2+...
     &                          +  0.300 * RXRAT(  67 )      ! CRES+OH=0.3*OPEN+0.6*XO2+...
      LOSS( OPEN             ) =           RXRAT(  71 )      ! OPEN+OH=XO2+2*CO+2*HO2+...
     &                          +          RXRAT(  72 )      ! OPEN=C2O3+HO2+CO
     &                          +          RXRAT(  73 )      ! OPEN+O3=0.03*ALD2+...

      PROD( CRO              ) =   0.400 * RXRAT(  67 )      ! CRES+OH=0.4*CRO+0.6*XO2+...
     &                          +          RXRAT(  68 )      ! CRES+NO3=CRO+HNO3+CSLAER
      LOSS( CRO              ) =           RXRAT(  69 )      ! CRO+NO2=NTR

      PROD( XYL              ) =   0.0
      LOSS( XYL              ) =           RXRAT(  70 )      ! XYL+OH=0.7*HO2+0.5*XO2+...

      PROD( MGLY             ) =   0.800 * RXRAT(  70 )      ! XYL+OH=0.8*MGLY+0.5*XO2+...
     &                          +  0.200 * RXRAT(  73 )      ! OPEN+O3=0.2*MGLY+0.62*C2O3+...
     &                          +  0.168 * RXRAT(  89 )      ! ISPD+OH=0.168*MGLY+...
     &                          +  0.850 * RXRAT(  90 )      ! ISPD+O3=0.85*MGLY+...
      LOSS( MGLY             ) =           RXRAT(  74 )      ! MGLY+OH=XO2+C2O3
     &                          +          RXRAT(  75 )      ! MGLY=C2O3+HO2+CO

      PROD( ISOP             ) =   0.0
      LOSS( ISOP             ) =           RXRAT(  76 )      ! ISOP+O=0.75*ISPD+0.5*FORM+...
     &                          +          RXRAT(  77 )      ! ISOP+OH=0.912*ISPD+...
     &                          +          RXRAT(  78 )      ! ISOP+O3=0.65*ISPD+0.6*FORM+...
     &                          +          RXRAT(  79 )      ! ISOP+NO3=0.2*ISPD+0.8*NTR+...
     &                          +          RXRAT(  93 )      ! ISOP+NO2=0.2*ISPD+0.8*NTR+...

      PROD( ISPD             ) =   0.750 * RXRAT(  76 )      ! ISOP+O=0.75*ISPD+0.5*FORM+...
     &                          +  0.912 * RXRAT(  77 )      ! ISOP+OH=0.912*ISPD+...
     &                          +  0.650 * RXRAT(  78 )      ! ISOP+O3=0.65*ISPD+0.6*FORM+...
     &                          +  0.200 * RXRAT(  79 )      ! ISOP+NO3=0.2*ISPD+0.8*NTR+...
     &                          +  0.200 * RXRAT(  93 )      ! ISOP+NO2=0.2*ISPD+0.8*NTR+...
      LOSS( ISPD             ) =           RXRAT(  89 )      ! ISPD+OH=1.565*PAR+...
     &                          +          RXRAT(  90 )      ! ISPD+O3=0.114*C2O3+...
     &                          +          RXRAT(  91 )      ! ISPD+NO3=0.357*ALD2_SUR+...
     &                          +          RXRAT(  92 )      ! ISPD=0.333*CO+...

      PROD( SO2              ) =   0.0
      LOSS( SO2              ) =           RXRAT(  83 )      ! SO2+OH=SULF+HO2+SULAER
     &                          +          RXRAT(  84 )      ! SO2=SULF+SULAER

      PROD( SULF             ) =           RXRAT(  83 )      ! SO2+OH=SULF+HO2+SULAER
     &                          +          RXRAT(  84 )      ! SO2=SULF+SULAER
      LOSS( SULF             ) =   0.0

      PROD( UMHP             ) =           RXRAT(  85 )      ! XO2+HO2=UMHP
      LOSS( UMHP             ) =   0.0

      PROD( ACROLEIN         ) =   0.850 * RXRAT(  99 )      ! BUTADIENE13+OH=...
     &                          +  1.900 * RXRAT( 100 )      ! BUTADIENE13+O3=...
     &                          +  0.230 * RXRAT( 102 )      ! BUTADIENE13+O=...
      LOSS( ACROLEIN         ) =           RXRAT(  94 )      ! ACROLEIN+OH=0.709*FORM+...
     &                          +          RXRAT(  95 )      ! ACROLEIN+O3=...
     &                          +          RXRAT(  96 )      ! ACROLEIN+NO3=1.192*XO2+...
     &                          +          RXRAT(  97 )      ! ACROLEIN+O=0.538*ALD2_SUR+...
     &                          +          RXRAT(  98 )      ! ACROLEIN=6.3*XO2+12.6*HO2+...

      PROD( BUTADIENE13      ) =   0.0
      LOSS( BUTADIENE13      ) =           RXRAT(  99 )      ! BUTADIENE13+OH=0.85*FORM+...
     &                          +          RXRAT( 100 )      ! BUTADIENE13+O3=...
     &                          +          RXRAT( 101 )      ! BUTADIENE13+NO3=0.14*XO2+...
     &                          +          RXRAT( 102 )      ! BUTADIENE13+O=...

      PROD( FORM_SUR         ) =   0.0
      LOSS( FORM_SUR         ) =           RXRAT( 107 )      ! FORM_SUR+OH=HO2+CO
     &                          +          RXRAT( 108 )      ! FORM_SUR=2*HO2+CO
     &                          +          RXRAT( 109 )      ! FORM_SUR=CO
     &                          +          RXRAT( 110 )      ! FORM_SUR+O=OH+HO2+CO
     &                          +          RXRAT( 111 )      ! FORM_SUR+NO3=HNO3+HO2+CO

      PROD( FORM_PRIMARY     ) =   0.0
      LOSS( FORM_PRIMARY     ) =           RXRAT( 112 )      ! FORM_PRIMARY+OH=OH
     &                          +          RXRAT( 113 )      ! FORM_PRIMARY=
     &                          +          RXRAT( 114 )      ! FORM_PRIMARY=
     &                          +          RXRAT( 115 )      ! FORM_PRIMARY+O=O
     &                          +          RXRAT( 116 )      ! FORM_PRIMARY+NO3=NO3

      PROD( ALD2_PRIMARY     ) =   0.0
      LOSS( ALD2_PRIMARY     ) =           RXRAT( 117 )      ! ALD2_PRIMARY+O=O
     &                          +          RXRAT( 118 )      ! ALD2_PRIMARY+OH=OH
     &                          +          RXRAT( 119 )      ! ALD2_PRIMARY+NO3=NO3
     &                          +          RXRAT( 120 )      ! ALD2_PRIMARY=

      PROD( ACROLEIN_PRIMARY ) =   0.0
      LOSS( ACROLEIN_PRIMARY ) =           RXRAT( 121 )      ! ACROLEIN_PRIMARY+OH=OH
     &                          +          RXRAT( 122 )      ! ACROLEIN_PRIMARY+O3=O3
     &                          +          RXRAT( 123 )      ! ACROLEIN_PRIMARY+NO3=NO3
     &                          +          RXRAT( 124 )      ! ACROLEIN_PRIMARY+O=O
     &                          +          RXRAT( 125 )      ! ACROLEIN_PRIMARY=

      IF( L_AE_VRSN ) THEN

         PROD( TOLAER           ) =           RXRAT(  64 )      ! TOL+OH=TOLAER+0.36*CRES+...
         LOSS( TOLAER           ) =  0.0

         PROD( CSLAER           ) =           RXRAT(  67 )      ! CRES+OH=CSLAER+0.6*XO2+...
     &                             +          RXRAT(  68 )      ! CRES+NO3=CSLAER+HNO3+CRO
         LOSS( CSLAER           ) =  0.0

         PROD( XYLAER           ) =           RXRAT(  70 )      ! XYL+OH=XYLAER+0.5*XO2+...
         LOSS( XYLAER           ) =  0.0

         PROD( SULAER           ) =           RXRAT(  83 )      ! SO2+OH=SULAER+HO2+SULF
     &                             +          RXRAT(  84 )      ! SO2=SULAER+SULF
         LOSS( SULAER           ) =  0.0

         PROD( TERP             ) =  0.0
         LOSS( TERP             ) =           RXRAT( 126 )      ! TERP+OH=TERPAER+OH
     &                             +          RXRAT( 127 )      ! TERP+NO3=TERPAER+NO3
     &                             +          RXRAT( 128 )      ! TERP+O3=TERPAER+O3

         PROD( TERPAER          ) =           RXRAT( 126 )      ! TERP+OH=TERPAER+OH
     &                             +          RXRAT( 127 )      ! TERP+NO3=TERPAER+NO3
     &                             +          RXRAT( 128 )      ! TERP+O3=TERPAER+O3
         LOSS( TERPAER          ) =  0.0

      ENDIF

      PNEG( PAR ) =   2.100 * RXRAT(  54 )      ! ROR=-2.1*PAR+0.96*XO2+...
     &             +          RXRAT(  58 )      ! OLE+OH=-PAR+ALD2+XO2+HO2+FORM
     &             +          RXRAT(  59 )      ! OLE+O3=-PAR+0.74*FORM+...
     &             +          RXRAT(  60 )      ! OLE+NO3=-PAR+0.09*XO2N+...
     &             +  0.709 * RXRAT(  94 )      ! ACROLEIN+OH=-0.709*PAR+...
     &             + 20.651 * RXRAT(  95 )      ! ACROLEIN+O3=-20.651*PAR+...
     &             +  1.310 * RXRAT(  96 )      ! ACROLEIN+NO3=-1.31*PAR+...
     &             +  0.850 * RXRAT(  99 )      ! BUTADIENE13+OH=-0.85*PAR+...
     &             +  3.800 * RXRAT( 100 )      ! BUTADIENE13+O3=-3.8*PAR+...
     &             +  0.154 * RXRAT( 101 )      ! BUTADIENE13+NO3=-0.154*PAR+...

      RETURN

      END
